{
    "name": "WikiPop",
    "version": "1.0",
    "description": "Popup definitions for wikipedia links",
    "manifest_version": 2,
    "content_scripts": 
    [
        {
            "matches":
            [
              "<all_urls>" 
            ],
            "js": 
            [
                "jquery.js", 
                "hoverIntent.js", 
                "WikiPop.js"
            ]
        }
    ],
    
    "permissions": 
    [
        "<all_urls>"
    ]
}